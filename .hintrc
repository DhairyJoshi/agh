{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-align: -webkit-match-parent",
          "-webkit-text-size-adjust",
          "-moz-appearance",
          "backdrop-filter",
          "mask-position",
          "mask-size",
          "mask-image"
        ]
      }
    ]
  }
}